{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cb/hero",
  "version": "0.1.0",
  "title": "Hero",
  "category": "widgets",
  "icon": "superhero",
  "description": "a hero block",
  "example": {},
  "supports": {
    "html": false,
    "align": [
      "full"
    ]
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    },
    "videoName": {
      "type": "string"
    },
    "videoID": {
      "type": "number"
    },
    "videoURL": {
      "type": "string"
    },
    "mimeType": {
      "type": "string"
    }
  },
  "textdomain": "my-custom-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}